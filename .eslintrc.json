{
  "env": {
    "node": true,
    "jest": true
  },
  "extends":["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "prettier/prettier": ["error", {
      "tabWidth": 2,
      "useTabs": false,
      "semi": true,
      "singleQuote": true,
      "endOfLine": "auto"
    }],
    "prefer-arrow-callback": 2,
    "no-console": 1,
    "prefer-const": 2
  }
}
